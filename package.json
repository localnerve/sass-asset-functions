{
  "name": "@localnerve/sass-asset-functions",
  "version": "7.3.0",
  "description": "compass-style asset functions for dart-sass or other sass compilers",
  "main": "index.js",
  "exports": {
    "import": "./index.js",
    "require": "./cjs/index.cjs",
    "default": "./index.js"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "posttranspile": "node posttranspile.js",
    "prepublishOnly": "npm run transpile",
    "pretranspile": "rimraf ./cjs",
    "test": "npm run test:unit && npm run test:package",
    "test:package": "node ./__test-package__/index.js",
    "test:unit": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:unit:debug": "node --inspect-brk --experimental-vm-modules node_modules/./bin/test",
    "transpile": "babel index.js -o ./cjs/index.cjs && babel ./lib --out-dir ./cjs/lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/localnerve/sass-asset-functions.git"
  },
  "keywords": [
    "node",
    "sass",
    "dart-sass",
    "assets",
    "image",
    "image-url",
    "image-width",
    "image-height",
    "font-url",
    "font-files",
    "lookup"
  ],
  "author": "Alex Grant <alex@localnerve.com>",
  "maintainers": [
    {
      "name": "localnerve",
      "email": "alex@localnerve.com",
      "url": "https://www.localnerve.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/localnerve/sass-asset-functions/issues"
  },
  "homepage": "https://github.com/localnerve/sass-asset-functions",
  "dependencies": {
    "image-size": "^2.0.2",
    "immutable": "^5.1.3",
    "mime-types": "^3.0.1",
    "sass": "^1.93.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "eslint-plugin-jest": "^29.0.1",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "rimraf": "^6.0.1",
    "tar": "^7.4.3"
  },
  "engines": {
    "node": ">=20"
  }
}
