{
  "name": "@localnerve/sass-asset-functions",
  "version": "4.3.0",
  "description": "compass-style asset functions for dart-sass or other sass compilers",
  "main": "index.js",
  "exports": {
    "import": "./index.js",
    "require": "./cjs/index.cjs",
    "default": "./index.js"
  },
  "type": "module",
  "scripts": {
    "test-package": "node ./test-package/index.js",
    "test": "jest && npm run test-package",
    "lint": "eslint .",
    "pretranspile": "rimraf ./cjs",
    "transpile": "babel index.js -o ./cjs/index.cjs && babel ./lib/processor.js -o ./cjs/lib/processor.cjs",
    "posttranspile": "node posttranspile.cjs",
    "prepublishOnly": "npm run transpile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/localnerve/sass-asset-functions.git"
  },
  "keywords": [
    "node",
    "sass",
    "dart-sass",
    "assets",
    "image",
    "image-url",
    "image-width",
    "image-height",
    "font-url",
    "font-files"
  ],
  "author": "Alex Grant <alex@localnerve.com>",
  "maintainers": [
    {
      "name": "localnerve",
      "email": "alex@localnerve.com",
      "url": "https://github.com/localnerve"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/localnerve/sass-asset-functions/issues"
  },
  "homepage": "https://github.com/localnerve/sass-asset-functions",
  "dependencies": {
    "image-size": "^1.0.2",
    "mime": "^3.0.0",
    "sass": "^1.66.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/preset-env": "^7.22.14",
    "rimraf": "^5.0.1",
    "jest": "^29.6.4",
    "eslint": "^8.48.0",
    "node-sass": "^9.0.0",
    "tar": "^6.1.15",
    "glob": "^10.3.3"
  },
  "engines": {
    "node": ">= 16"
  }
}
